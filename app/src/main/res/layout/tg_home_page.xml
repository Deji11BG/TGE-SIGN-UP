<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.babbangona.tgesign_up.TFMHome"
    android:orientation="vertical">

    <include layout="@layout/tg_home_content" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="bottom"
        android:padding="@dimen/tfm_text_view_padding">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/btn_add_new_member"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/view_blue"
            android:text="@string/add_new_member"
            android:layout_gravity="bottom" />

    </LinearLayout>

    <!-- Adding bottom sheet after main content -->
    <!--
    <include layout="@layout/leader_registration_confirmation_dialog" />-->

</androidx.coordinatorlayout.widget.CoordinatorLayout>