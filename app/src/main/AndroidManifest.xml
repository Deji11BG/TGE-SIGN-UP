<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tgesign_up">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TGHome"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TFMHome"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FormMemberInformation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FormMemberLocation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TGPage.TgMembers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VerifyTemplate"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CaptureTemplate"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CaptureActivity" />
        <activity
            android:name=".VerifyActivity" />
        <activity
            android:name=".ScheduleInfo"
            android:theme="@style/AppTheme.NoActionBar" />/>
    </application>

</manifest>